{
  "headers": {
    "/*": {
      "X-Author": "neveraskedfor",
      "Content-Type": "text/plain; charset=UTF-8",
      "Access-Control-Allow-Origin": "*"
    },
    "/sample/**": {
      "X-Author": "neveraskedfor",
      "Content-Type": "text/plain; charset=UTF-8",
      "Access-Control-Allow-Origin": "*"
    },
    "/login/*": {
      "X-Author": "neveraskedfor",
      "Content-Type": "text/plain; charset=UTF-8",
      "Access-Control-Allow-Origin": "*"
    }
  },
  "clean_urls": true,
  "routes": {
    "/login/**": "index.html",
    "/sample/**": "sample"
  }
}
